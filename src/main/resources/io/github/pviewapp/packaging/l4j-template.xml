<launch4jConfig>
    <headerType>gui</headerType>

    <dontWrapJar>true</dontWrapJar>

    <icon>%s</icon>
    <outfile>%s</outfile>

    <classPath>
        <mainClass>%s</mainClass>
        <cp>.</cp>
    </classPath>

    <jre>
        <path>%%EXEDIR%%</path>
        <bundledJre64Bit>true</bundledJre64Bit>

        %s
    </jre>


    <chdir>.</chdir>
</launch4jConfig>